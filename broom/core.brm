(define defmacro
        (macro [name xs body]
               (list 'define name (list 'macro xs body))))

(defmacro identity [expr]
    expr)

(defmacro when [expr form]
    (list 'if expr form '()))

(defmacro lambda [xs body]
    (list 'fn (List2Arr xs) body))

(defmacro defn [name args body]
    (list 'define name (list 'fn args body)))


(defn even? [x]
    (= (mod x 2) 0))

(defn zero? [x]
    (= x 0))

